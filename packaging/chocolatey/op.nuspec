<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>op</id>
    <version>0.1.0</version>
    <title>op (Application Launcher)</title>
    <authors>Adarsh</authors>
    <owners>Adarsh</owners>
    <projectUrl>https://github.com/Adarsh-codesOP/op-cli</projectUrl>
    <tags>op launcher cli productivity</tags>
    <summary>A fast, fuzzy-matching application launcher for Windows.</summary>
    <description>
      op is a lightweight CLI utility that finds and launches applications instantly.
      
      Features:
      - Fuzzy search (type 'code' to open 'Visual Studio Code')
      - Learning (remembers your past choices)
      - Offline (no internet required)
      - Single binary (no dependencies)
    </description>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
